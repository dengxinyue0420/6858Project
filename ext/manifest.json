{
	"manifest_version": 2,
	"name": "CertMonitor Extension",
	"version": "1.1",
	"permissions": [
		"https://*/*",
		"contextMenus",
		"tabs",
		"webRequest",
		"management",
		"webRequestBlocking"
	],
	"background": {
	    "scripts": ["URI.js", "underscore-min.js", "background.js"],
	    "persistent": true
	},
	"browser_action": {
	    "default_icon": "icon.png",
	    "default_popup": "popup.html"
	}
}
